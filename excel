public class PetCo1 {

        WebDriver driver;

@BeforeTest

public void setup() throws Exception{

        System.setProperty("webdriver.chrome.driver", "C:\\softwares\\Selenium java\\chromedriver.exe");

        driver = new ChromeDriver();



        // Maximize the window

        driver.manage().window().maximize();



        // Delete all cookies

        driver.manage().deleteAllCookies();



        //Add wait time implicitly and open the URL

        driver.manage().timeouts().implicitlyWait(5000,TimeUnit.SECONDS);



        // Open the URL

        driver.get("https://www.petco.com");

        Thread.sleep(10000);

}



public void Screenshot(String fileName) throws IOException{



                //1. Take screenshot and Store it in a file format

        File file=((TakesScreenshot)driver).getScreenshotAs(OutputType.FILE);



        //2. Now copy the screenshot to desired location using copyfile method :

        FileUtils.copyFile(file, new File("/Users/759412/Desktop/OSCAR/RDP/Selenium SBA/SeleniumSBA/Screenshots/"+fileName+".jpg"));



}

public void writeexcel(String Filepath , String Filename,String sheetname, String data ) throws Exception{



        //Create an object of file class to open xlsx file

        File file = new File(Filepath+"\\"+Filename);



        //Create an object of fileInputstream class to read excel file

        FileInputStream inputstream = new FileInputStream(file);





        XSSFWorkbook wb = new XSSFWorkbook(inputstream);



        //Read excel sheet by sheet name

        Sheet sheet = wb.createSheet(sheetname);



        //get the current count of rows in excel file

        int rowcount = sheet.getLastRowNum()- sheet.getFirstRowNum();



        //get first row from the sheet



        Row newrow = sheet.createRow(rowcount+1);



        //Fill the data in the row



        Cell cell = newrow.createCell(0);

        cell.setCellValue(data);



   //close inputstream

        inputstream.close();



        //create an output of FileOutputStream class to create write data in excel sheet



        FileOutputStream outputstream = new FileOutputStream(file);

        wb.write(outputstream);

        outputstream.close();





}



