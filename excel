package Excel91;







import java.io.File;



import java.io.FileInputStream;



import java.io.FileOutputStream;



import java.io.IOException;



import org.apache.poi.xssf.usermodel.XSSFSheet;



import org.apache.poi.xssf.usermodel.XSSFWorkbook;







public class Excel {



 public static String value1;



 public static String value2;



 public static String value3;



 public static String getvalue1() throws IOException



 {



 FileInputStream f = new FileInputStream("C:\\Selenium Excel\\91Mobiles.xlsx");



 XSSFWorkbook wb = new XSSFWorkbook(f);



 XSSFSheet sh =wb.getSheet("Sheet1");



 sh.getRow(0).getCell(0).getStringCellValue();



 return value1;



 }



 public static String getValue2() throws IOException



 {



 FileInputStream fi = new FileInputStream("C:\\Selenium Excel\\91Mobiles.xlsx");



 XSSFWorkbook wb = new XSSFWorkbook(fi);



 XSSFSheet sh =wb.getSheet("Sheet1");



  sh.getRow(0).getCell(1).getNumericCellValue();



 return value2;



 }



 public static String getValue3(String w) throws IOException



 {



 File fe = new File("C:\\Selenium Excel\\91Mobiles.xlsx");



 FileInputStream fis = new FileInputStream(fe);



 XSSFWorkbook wb = new XSSFWorkbook(fis);



 XSSFSheet sh = wb.getSheet("Sheet1");



 sh.createRow(0).createCell(2).setCellValue(w);



 FileOutputStream fos = new FileOutputStream("C:\\Selenium Excel\\91Mobiles.xlsx");



 wb.write(fos);



 return value3;



}}







