import org.openqa.selenium.TakesScreenshot;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.chrome.ChromeDriver;

import org.openqa.selenium.interactions.Actions;

import org.openqa.selenium.support.ui.Select;

import org.testng.annotations.AfterTest;

import org.testng.annotations.BeforeTest;

import org.testng.annotations.Test;





public class Marshalpetzonefinal {

        WebDriver driver;

        @BeforeTest

        public void setup() throws Exception{



           //Set system property

                System.setProperty("webdriver.chrome.driver", "C:\\softwares\\Selenium java\\chromedriver.exe");

                driver = new ChromeDriver();



                //Maximize the window

                driver.manage().window().maximize();



                //Delete all the cookies

                driver.manage().deleteAllCookies();



                //Giving implicit wait for opening the browser

                driver.manage().timeouts().implicitlyWait(8000, TimeUnit.SECONDS);



                //Open the Url

                driver.manage().timeouts().pageLoadTimeout(30, TimeUnit.SECONDS);

                driver.get("https://www.marshallspetzone.com/");

                Thread.sleep(6000);



        }



        public void Screenshot( String filename) throws Exception{

                //Type casting driver into takesscreenshot and take screenshot and store it in file format

                File file = ((TakesScreenshot)driver).getScreenshotAs(OutputType.FILE);



                //Now we need to copy the screen shot to desired location

                FileUtils.copyFile(file, new File("/Users/759412/Desktop/OSCAR/RDP/Selenium SBA/SeleniumSBA/ClearTripScreen/"+filename+".jpg"));

        }







        @Test

        public void selectMaincategory() throws Exception{



                //Click on Main category as cat and then sub category as accessories

                WebElement Cat = driver.findElement(By.xpath("//*[@id='cbp-hrmenu-tab-13']/a/span"));

                WebElement Accessories = driver.findElement(By.xpath("//*[@id='cbp-hrmenu-tab-13']/div/div/div/div[1]/div[3]/div/div/div/div/a"));





                Actions action = new Actions(driver);

                action.moveToElement(Cat).build().perform();

                Thread.sleep(3000);

                action.moveToElement(Accessories).build().perform();

                Accessories.click();

                Thread.sleep(5000);

                System.out.println("Accessories has been loaded");

                //Screenshot("Subcategory_Accessories");

                Thread.sleep(3000);



                //To sort the page for Price,low to high

                WebElement Sort = driver.findElement(By.xpath("//*[@id='js-product-list-top']/div/div[3]/div[1]/a"));



                action.moveToElement(Sort).build().perform();

                Sort.click();

                WebElement price = driver.findElement(By.xpath("//*[@id='js-product-list-top']/div/div[3]/div[1]/div/a[4]"));

                action.moveToElement(price).build().perform();

                Sort.click();











                ////*[@id="js-product-list-top"]/div/div[3]/div[1]/a

        }

        //@AfterTest

        //public void TearDown(){

                //driver.close();

        }



