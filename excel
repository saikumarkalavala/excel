package com.datadriventest;



import java.io.FileInputStream;

import java.io.FileNotFoundException;



import org.apache.poi.xssf.usermodel.XSSFSheet;

import org.apache.poi.xssf.usermodel.XSSFWorkbook;



public class ExcelAxisBank {



        public static String getAmount() throws Exception{



                FileInputStream fis = new FileInputStream("C:\\softwares\\Oscar\\SeleniumSBA\\AxisBank.xlsx");

                XSSFWorkbook wb = new XSSFWorkbook(fis);

                XSSFSheet sh = wb.getSheetAt(0);



                String Amount = sh.getRow(0).getCell(0).getRawValue();



                return Amount;



        }



}

package com.datadriventest;



import java.io.File;

