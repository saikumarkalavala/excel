 public static String returndate = "(//*[contains(@class,'ui-state-default ng-tns-c11-3')])[21]";

 public static String carbutton = "//*[contains(@class,'book-button btn')]";

public static String pickuptime= "//*[@id='pickUpTime']";

public static String time="//*[@id='pickUpTime']/option[@value='2019-06-20T04:00:00+05:30']"

}

package testng;



import org.testng.annotations.Test;

import savaari.Driversetup;

import xlsetup.XLsetup;

import org.testng.annotations.BeforeTest;

import path.Paths;

import java.io.File;

import java.util.concurrent.TimeUnit;

import org.apache.commons.io.FileUtils;

import org.openqa.selenium.By;

import org.openqa.selenium.OutputType;

import org.openqa.selenium.TakesScreenshot;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.support.ui.Select;

import org.testng.annotations.AfterTest;



public class NewTest {

 public static WebDriver d;

 public static void Wait(){

 d.manage().timeouts().implicitlyWait(5000, TimeUnit.MILLISECONDS);

 }

 public static void Screenshot(String n) throws Exception{

 File src = ((TakesScreenshot)d).getScreenshotAs(OutputType.FILE);

 File dst = new File("Screenshots/"+n+".png");

 FileUtils.copyFile(src, dst);

 }

 @Test

 public void search() throws Exception {

  d.findElement(By.xpath("//label[contains(@class,'margin-left-20px')]//span")).click();

  WebElement we = d.findElement(By.xpath(Paths.fromcity));

  we.click();

  xlsetup.XLsetup.getvalue1(0,0);

  Wait();

  we.sendKeys(XLsetup.value1);



 WebElement we1 = d.findElement(By.xpath(Paths.tocity));

  we1.click();

  xlsetup.XLsetup.getvalue2(1,1);

  Wait();

  we1.sendKeys(XLsetup.value2);



  d.findElement(By.xpath("(//*[contains(@class,'ng-tns-c11-1 form-control ui-inputtext ui-widget ui-state-default ui-corner-all')])")).click();

  d.findElement(By.xpath(Paths.ondate)).click();

  Wait();



  d.findElement(By.xpath("//*[contains(@class,'ng-tns-c11-3 form-control ui-inputtext ui-widget ui-state-default ui-corner-all')]")).click();

  d.findElement(By.xpath(Paths.returndate)).click();

  Wait();



  Select s = new Select(d.findElement(By.id("pickUpTime")));

  s.selectByIndex(5);

  Wait();



  d.findElement(By.xpath(Paths.carbutton)).click();

  Wait();



  Screenshot("cars");

 }

 @BeforeTest

 public void beforeTest() {

  d = Driversetup.chromesetup();

  String url = "https://www.savaari.com/";

  d.get(url);

  d.manage().window().maximize();

  Wait();

 }



 @AfterTest

 public void afterTest() {

 d.quit();

 }





}





