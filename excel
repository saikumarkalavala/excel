import org.openqa.selenium.interactions.Actions;

import org.openqa.selenium.support.ui.Select;

import org.testng.Assert;

import org.testng.annotations.AfterTest;

import org.testng.annotations.BeforeTest;

import org.testng.annotations.Test;



public class CleatTripFlights extends Excel {

        WebDriver driver;



        @BeforeTest

        public void Stepup() throws Exception {



                // To set chrome driver

                System.setProperty("webdriver.chrome.driver", "C:\\softwares\\Selenium java\\chromedriver.exe");

                driver = new ChromeDriver();



                // To maximize the window



                driver.manage().window().maximize();



                // TO delete all the cookies



                driver.manage().deleteAllCookies();



                // TO add wait implicitly



                driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);



                // TO open the URL



                driver.get("https://www.cleartrip.com/flights");

                Thread.sleep(3000);



        }



        public void Screenshot(String Filename) throws Exception {



                // To take Screenshot and store in into file format



                File file = ((TakesScreenshot) driver).getScreenshotAs(OutputType.FILE);



                // Copy the screenshot into desired location by copyfile method

                FileUtils.copyFile(file, new File("C:/Users/759412/Desktop/OSCAR/RDP/SELENI~1/SeleniumSBA/ClearTripScreen/" + Filename + ".jpg"));



        }



        @Test(priority = 1)

        public void roundtrip() throws Exception {



                WebElement roundtrip = driver.findElement(By.xpath("//*[@id='RoundTrip']"));



                Actions action = new Actions(driver);

                action.moveToElement(roundtrip).build().perform();

                roundtrip.click();



                // Take screenshot of the page

                Screenshot("RoundTrip");

        }



        @Test(priority = 2)

        public void Enter_FromandTo() throws Exception {



                System.out.println(getvalue1());

                System.out.println(getvalue2());

                WebElement From = driver.findElement(By.xpath("//*[@id='FromTag']"));

                From.sendKeys(getvalue1());

                From.sendKeys(Keys.DOWN);

                From.sendKeys(Keys.TAB);



                WebElement To = driver.findElement(By.xpath("//*[@id='ToTag']"));

                To.sendKeys(getvalue2());

                To.sendKeys(Keys.DOWN);

                To.sendKeys(Keys.ENTER);



                // Departure Date

                driver.findElement(By.xpath("//*[@id='DepartDate']")).click();

                driver.findElement(By.xpath("//*[text()='26']")).click();



                // Return Date

                driver.findElement(By.xpath("//*[@id='ReturnDate']")).clear();

                driver.findElement(By.xpath("//*[@id='ui-datepicker-div']/div[1]/table/tbody/tr[5]/td[4]/a")).click();



                // Select 2 adults

                Select Adult = new Select(driver.findElement(By.xpath("//*[@id='Adults']")));

                Adult.selectByIndex(1);





                // Click on Search Flights

                WebElement SearchFlight = driver.findElement(By.xpath("//*[@id='SearchBtn']"));

                SearchFlight.click();

                Thread.sleep(4000);



                // To take screenshot of flight search

                Screenshot("FlightSearch3");

                Thread.sleep(2000);

                System.out.println("Screenshot taken ");



        }



        @AfterTest

        public void TearUp() {

        driver.close();

        }



}





Excel for clear trip



package com.ClearTrip;



import java.io.FileInputStream;

import java.io.IOException;



import org.apache.poi.xssf.usermodel.XSSFSheet;

import org.apache.poi.xssf.usermodel.XSSFWorkbook;



public class Excel {



        public static String getvalue1() throws Exception {

                FileInputStream fis = new FileInputStream("C:\\softwares\\Jar files\\ClearTrip.xlsx");

                XSSFWorkbook wb = new XSSFWorkbook(fis);

                XSSFSheet sh = wb.getSheetAt(0);



                String Value1 = sh.getRow(0).getCell(0).getStringCellValue();



                return Value1;

        }



        public static String getvalue2() throws Exception {

                FileInputStream fi = new FileInputStream("C:\\softwares\\Jar files\\ClearTrip.xlsx");

                XSSFWorkbook wb = new XSSFWorkbook(fi);

                XSSFSheet sh = wb.getSheetAt(0);



                String Value2 = sh.getRow(0).getCell(1).getStringCellValue();

                return Value2;



        }



}



