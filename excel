goindigo:



public class DriverSetup {



    static WebDriver driver=null;



    public static String project_path=System.getProperty("user.dir");



    static String browser_path=null;



    public static WebDriver createDriver() throws IOException



    {



       FileInputStream file =new FileInputStream(project_path+"//Global_Settings.properties");



       Properties property =new Properties();



       property.load(file);



       String browser_Name=property.getProperty("browserName");



       if(browser_Name.equals("Chrome"))



       {



           browser_path=project_path+"//browsers//chromedriver.exe";



           System.setProperty("webdriver.chrome.driver",browser_path);



           ChromeOptions options = new ChromeOptions();



              options.addArguments("--disable-notifications");







           driver=new ChromeDriver();



       }



       else if(browser_Name.equals("Firefox"))



       {



           browser_path=project_path+"//browsers//geckodriver.exe";



           System.setProperty("webdriver.firefox.driver",browser_path);



           driver=new FirefoxDriver();



       }



       return driver;



    }



}



public class excel {



    public static String getvalue(int row,int col) throws IOException



    {



       FileInputStream f = new FileInputStream(DriverSetup.project_path+"\\goindio.xls");



       HSSFWorkbook wb = new HSSFWorkbook(f);



       HSSFSheet sh = wb.getSheet("sheet1");



       String value = sh.getRow(row).getCell(col).getStringCellValue();



    // wb.close();



       return value;







    }

}

public class xpath {



    public static String triptype ="//label[@for='roundTrip']";



  public static String from ="//input[@name='or-src']";



  public static String to ="//input[@name='or-dest']";



  public static String deparuture_date ="//*[@class='ig-input-group field-float cal-focus']/input";



  public static String return_date = "//*[@class='ig-input-group field-float cal-focus']/input";



  public static String click_passengers="//*[@name='passenger']";



  public static String add_passengers="//label[contains(text(),'Adult')]//following::div[1]/button[2]";



  public static String search = "//span[@class='hp-src-btn']";



  public static String click_Done="//*[@class='btn btn-primary pax-done btn-pad-y']";



}



















public class code {



    public static WebDriver driver;







    public static void depart_arrival_Date(String input_date) {



       System.out.println("-->" + input_date);



       List<WebElement> table_cols = driver.findElements(By.xpath("//div[@class='ui-datepicker-group ui-datepicker-group-last']/table/tbody/tr/td"));



       for (WebElement element : table_cols) {



           String depart_date = element.getText();



           if (depart_date.contains(input_date)) {



              System.out.println("Date Clicked");



              System.out.println("Yes" + input_date);



              element.click();



              break;



           }



       }



    }







    @Test



    public void A() throws Exception {



       driver.get("https://www.goindigo.in/");



       driver.manage().window().maximize();



       driver.manage().timeouts().pageLoadTimeout(30, TimeUnit.SECONDS);



       driver.navigate().refresh();



       driver.findElement(By.xpath(xpath.triptype)).click();



       driver.manage().timeouts().implicitlyWait(20, TimeUnit.SECONDS);



       System.out.print(excel.getvalue(1, 0) + " " + excel.getvalue(1, 1));



       driver.findElement(By.xpath(xpath.from)).click();



       driver.findElement(By.xpath(xpath.from)).sendKeys(excel.getvalue(1, 0));



       Thread.sleep(2000);



       driver.findElement(By.xpath(xpath.from)).sendKeys(Keys.ENTER);



       driver.findElement(By.xpath(xpath.to)).click();



       driver.findElement(By.xpath(xpath.to)).sendKeys(excel.getvalue(1, 1));



       driver.findElement(By.xpath(xpath.to)).sendKeys(Keys.ENTER);



       Thread.sleep(2000);



       code.depart_arrival_Date("15");



       Thread.sleep(5000);



       code.depart_arrival_Date("20");



       driver.findElement(By.xpath(xpath.click_passengers)).click();



       driver.findElement(By.xpath(xpath.add_passengers)).click();



       driver.findElement(By.xpath(xpath.click_Done)).click();



       driver.findElement(By.xpath(xpath.search)).click();



       driver.manage().timeouts().pageLoadTimeout(15, TimeUnit.SECONDS);



       File s = ((TakesScreenshot) driver).getScreenshotAs(OutputType.FILE);



       FileUtils.copyFile(s, new File(DriverSetup.project_path + "//goibibo.png"));



    }







    @BeforeTest



    public void beforeTest() throws IOException {



       driver = DriverSetup.createDriver();



    }







    @AfterTest



    public void afterTest() {



       driver.quit();



    }







}

