//driver.manage().timeouts().wait();



 driver.findElement(By.xpath("//input[@id='src']")).clear();



 driver.findElement(By.xpath("//input[@id='src']")).sendKeys(excelsetup.readexcel(2,0));



 Thread.sleep(1000);



 driver.findElement(By.xpath("//input[@id='src']")).sendKeys(Keys.ENTER);



 driver.findElement(By.xpath("//input[@id='dest']")).clear();



 driver.findElement(By.xpath("//input[@id='dest']")).sendKeys(excelsetup.readexcel(2,1));



 Thread.sleep(1000);



 driver.findElement(By.xpath("//input[@id='dest']")).sendKeys(Keys.ENTER);



 Thread.sleep(3000);



 driver.findElement(By.xpath("//label[@for='onward_cal']")).click();



 driver.findElement(By.xpath("(//*[@class='wd day'])[19]")).click();



 driver.findElement(By.xpath("//label[@for='return_cal']")).click();



 driver.findElement(By.xpath("(//*[@class='wd day'])[24]")).click();



 Thread.sleep(3000);



 driver.findElement(By.xpath("//button[@id='search_btn']")).click();



 driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);



 driver.findElement(By.xpath("//label[@for='bt_AC'][1]")).click();



 Thread.sleep(1000);



 TakesScreenshot fk=(TakesScreenshot)driver;



 File f1=fk.getScreenshotAs(OutputType.FILE);



 File f= new File("E:\\selenium\\shot2.png");



 FileUtils.copyFile(f1, f);



