package sel2;







import java.io.File;



import java.io.IOException;



import java.util.Set;







import org.apache.commons.io.CopyUtils;



import org.apache.commons.io.FileUtils;



import org.openqa.selenium.JavascriptExecutor;



import org.openqa.selenium.OutputType;



import org.openqa.selenium.TakesScreenshot;



import org.openqa.selenium.WebDriver;



import org.openqa.selenium.WebElement;



import org.openqa.selenium.support.ui.Select;







public class axisbank extends driversetup{







public static void main(String[] args) throws InterruptedException, IOException {



// TODO Auto-generated method stub



 //WebDriver driver;



 driver=driversetup.chromesetup();



 Thread.sleep(3000);



 driver.findElement(pom.gd).click();



 Thread.sleep(3000);



 driver.findElement(pom.emi).click();



 Thread.sleep(3000);



 JavascriptExecutor js = (JavascriptExecutor) driver;



 js.executeScript("window.scrollBy(0,500)");



 driver.navigate().refresh();



 Thread.sleep(3000);



 driver.findElement(pom.others).click();



 Thread.sleep(3000);



 TakesScreenshot fk=(TakesScreenshot)driver;



File f1=fk.getScreenshotAs(OutputType.FILE);



File f2=new File("E:\\selenium\\shot2.png");



FileUtils.copyFile(f1, f2);



 Select s= new Select(driver.findElement(pom.dp));







 String ss=excelsetup.readexcel(1,0);



 System.out.println(ss);



 s.selectByValue(ss);



 Thread.sleep(3000);



 driver.findElement(pom.know).click();



 Thread.sleep(3000);



 String title=driver.getTitle();



 String parentId=driver.getWindowHandle();



 Set <String> w=driver.getWindowHandles();



 for(String i:w) {



 String atitle=driver.switchTo().window(i).getTitle();



 if(atitle.equalsIgnoreCase(title)) {



 }



 else {



 driver.switchTo().window(i);



 String text=driver.findElement(pom.offer).getText();



 System.out.println(text);



 }











 }



 // JavascriptExecutor js1 = (JavascriptExecutor) driver;



// js1.executeScript("window.scrollBy(0,250)");



 String text=driver.findElement(pom.offer).getText();



 String text1=excelsetup.excelwrite(text,2,1);



 System.out.println(text);







 driver.quit();



}







}



