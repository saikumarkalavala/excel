package xlsetup;



import java.io.FileInputStream;

import java.io.IOException;



import org.apache.poi.xssf.usermodel.XSSFSheet;

import org.apache.poi.xssf.usermodel.XSSFWorkbook;



public class XLsetup {

 public static int value1;

 public static int value2;

 public static int getvalue1(int row,int col) throws Exception{

 FileInputStream fis = new FileInputStream("D:\\Selenium SBA\\icici\\Book1.xlsx");

 XSSFWorkbook wb = new XSSFWorkbook(fis);

 XSSFSheet sh = wb.getSheetAt(0);

 value1 = (int) sh.getRow(row).getCell(col).getNumericCellValue();

 return value1;



 }

 public static int getvalue2(int row1,int col1) throws Exception{

 FileInputStream fis = new FileInputStream("D:\\Selenium SBA\\icici\\Book1.xlsx");

 XSSFWorkbook wb1 = new XSSFWorkbook(fis);

 XSSFSheet sh1 = wb1.getSheetAt(0);

 value2 = (int) sh1.getRow(row1).getCell(col1).getNumericCellValue();

 return value2;



 }

}









package testng;



import org.testng.annotations.Test;

import org.testng.annotations.Test;

import path.Paths;

import org.testng.annotations.BeforeTest;

import java.io.File;

import java.util.List;

import java.util.concurrent.TimeUnit;

import org.apache.commons.io.FileUtils;

import org.openqa.selenium.By;

import org.openqa.selenium.JavascriptExecutor;

import org.openqa.selenium.OutputType;

import org.openqa.selenium.TakesScreenshot;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.interactions.Actions;

import org.openqa.selenium.support.ui.ExpectedConditions;

import org.openqa.selenium.support.ui.Select;

import org.openqa.selenium.support.ui.WebDriverWait;

import org.testng.annotations.AfterTest;



public class NewTest {

 public static WebDriver d;

 public static void Wait(){

 d.manage().timeouts().implicitlyWait(5000, TimeUnit.MILLISECONDS);

 }

 public static void Screenshot(String n) throws Exception{

 File src = ((TakesScreenshot)d).getScreenshotAs(OutputType.FILE);

 File dst = new File("Screenshots/"+n+".png");

 FileUtils.copyFile(src, dst);

 }





 @Test

 public void search() throws Exception {



 //WebDriverWait wait = new WebDriverWait(d,30);

 //WebElement me = wait.until(ExpectedConditions.visibilityOfElementLocated(By.id("paymframe")));

  boolean value = false;

  while(value==false){

  value =d.findElement(By.id("paymframe")).isDisplayed();

 }

  WebElement element=d.findElement(By.id("paymframe"));

  Actions action = new Actions(d); action.moveToElement(element).click().perform();

 d.switchTo().frame("paymframe");

   Wait();

   WebElement element1=d.findElement(By.xpath("//select[@id='ddlTypeOfFixedDeposit']/option[@value='Quarterly Payout']"));

  action.moveToElement(element1).click().perform();

  Select s = new Select(d.findElement(By.id("ddlTypeOfFixedDeposit")));

  s.selectByIndex(2);

  Wait();



  WebElement we = d.findElement(By.xpath("//input[@id='loanAmount']"));

  we.click();

  we.clear();

  int x = xlsetup.XLsetup.getvalue1(0, 0);

  String amt = Integer.toString(x);

  we.sendKeys(amt);

  Wait();



  d.findElement(By.xpath(Paths.radiobutton)).click();

  Wait();



  WebElement we1 = d.findElement(By.xpath(Paths.days));

  we1.click();

  we1.clear();

  int y =xlsetup.XLsetup.getvalue2(0, 1);

  String day = Integer.toString(y);

  we1.sendKeys(day);

  Wait();



  d.findElement(By.xpath("(//*[contains(@class,'col outAlignLeft')])[3]//span")).click();

  String money = d.findElement(By.xpath("(//*[contains(@class,'col outAlignLeft')])[4]//span")).getText();

  System.out.println(money);



  d.switchTo().defaultContent();

 // d.findElement(By.xpath(("(//*[contains(@class,'Logo')])[2]"))).click();

  Screenshot("icici");







 }

 @BeforeTest

 public void beforeTest() {

  d = driversetup.Driversetup.chromesetup();

  String url = "https://www.icicibank.com/calculator/fd-calculator.page";

  d.get(url);

  d.manage().window().maximize();

  d.manage().deleteAllCookies();



  Wait();

 }



 @AfterTest

 public void afterTest() {

  d.quit();

 }





}





