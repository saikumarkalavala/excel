Amresh

 public static String value1;

 public static int value2;



 public static String getValue3(String w) throws IOException {

  try{

                           {







   File fe = new File("C:\\Selenium Excel\\91Mobiles.xlsx");







   FileInputStream fis = new FileInputStream(fe);







   XSSFWorkbook wb = new XSSFWorkbook(fis);







   XSSFSheet sh = wb.getSheet("Sheet1");







   sh.createRow(0).createCell(2).setCellValue(w);







   FileOutputStream fos = new FileOutputStream("C:\\Selenium Excel\\91Mobiles.xlsx");







   wb.write(fos);







   return value1;

