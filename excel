//application flow



package selenimprep;







import java.io.IOException;



import java.util.concurrent.TimeUnit;







import org.openqa.selenium.Keys;



import org.openqa.selenium.WebDriver;



import org.openqa.selenium.WebElement;



public class Application extends driversetup {



public static void app() throws IOException {



driver.manage().timeouts().implicitlyWait(5000, TimeUnit.SECONDS);



WebElement round1=driver.findElement(pom.round);



round1.click();



driver.findElement(pom.from).click();



// driver.findElement(pom.from).sendKeys("BOM");



driver.findElement(pom.from).sendKeys(excelsetup.readexcel(0, 0));



driver.findElement(pom.from).sendKeys(Keys.ENTER);







WebElement to=driver.findElement(pom.to);



to.click();



to.sendKeys(excelsetup.readexcel(0, 1));



driver.findElement(pom.to).sendKeys(Keys.ENTER);







driver.findElement(pom.Departuredate).click();



driver.findElement(pom.Selectdate).click();



driver.findElement(pom.Done).click();



driver.findElement(pom.Returndate).click();



driver.findElement(pom.Selectdate1).click();



driver.findElement(pom.Done1).click();



driver.findElement(pom.Passenger).click();



driver.findElement(pom.Adults).click();



driver.findElement(pom.Adults).click();



driver.findElement(pom.Done2).click();



driver.findElement(pom.Payin).click();



driver.findElement(pom.IndianRupees).click();



driver.findElement(pom.SearchFlights).click();



driver.manage().timeouts().implicitlyWait(3000, TimeUnit.SECONDS);



driversetup.Screenshot("shotindigo");



String flightno=driver.findElement(pom.FFN).getText();



System.out.println(flightno);































//return driver;



}







}



