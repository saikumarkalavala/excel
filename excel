package savaari;



import org.openqa.selenium.WebDriver;

import org.openqa.selenium.chrome.ChromeDriver;



public class Driversetup {

 public static WebDriver driver;



 public static WebDriver chromesetup(){

 System.setProperty("webdriver.chrome.driver", "path");

 WebDriver driver = new ChromeDriver();

 return driver;



 }

}

package xlsetup;



import java.io.FileInputStream;

import org.apache.poi.xssf.usermodel.XSSFSheet;

import org.apache.poi.xssf.usermodel.XSSFWorkbook;



public class XLsetup {

 public static String value1;

 public static String value2;

 public static String getvalue1(int row,int col) throws Exception{

 FileInputStream fis = new FileInputStream("C:\\Users\\721143\\Desktop\\Excel\\Book.xlsx");

 XSSFWorkbook wb = new XSSFWorkbook(fis);

 XSSFSheet sh = wb.getSheetAt(0);

 value1 = sh.getRow(row).getCell(col).getStringCellValue();

 return value1;

 }

public static String getvalue2(int row1,int col1) throws Exception{

 FileInputStream fis = new FileInputStream("C:\\Users\\721143\\Desktop\\Excel\\Book.xlsx");

 XSSFWorkbook wb1 = new XSSFWorkbook(fis);

 XSSFSheet sh1 = wb1.getSheetAt(0);

 value1 = sh1.getRow(row1).getCell(col1).getStringCellValue();

 return value2;

 }



}

package path;



