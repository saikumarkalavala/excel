package com.datadriventest;



import java.io.File;

import java.io.IOException;



import java.util.concurrent.TimeUnit;



import org.apache.commons.io.FileUtils;

import org.openqa.selenium.By;

import org.openqa.selenium.JavascriptExecutor;

import org.openqa.selenium.Keys;

import org.openqa.selenium.OutputType;

import org.openqa.selenium.TakesScreenshot;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.chrome.ChromeDriver;

import org.openqa.selenium.interactions.Action;

import org.openqa.selenium.interactions.Actions;

import org.openqa.selenium.support.ui.ExpectedConditions;

import org.openqa.selenium.support.ui.Select;

import org.openqa.selenium.support.ui.WebDriverWait;

import org.testng.annotations.AfterTest;

import org.testng.annotations.BeforeTest;

import org.testng.annotations.Test;



public class AxisBankFinal extends ExcelAxisBank {



        WebDriver driver;



        @BeforeTest

        public void SetUp() throws Exception {



                // To set chrome driver

                System.setProperty("webdriver.chrome.driver", "C:\\softwares\\Selenium java\\chromedriver.exe");

                driver = new ChromeDriver();



                // TO maximixe the window

                driver.manage().window().maximize();



                // TO delete all cookies

                driver.manage().deleteAllCookies();



                // TO Wait implicitely

                driver.manage().timeouts().implicitlyWait(2000, TimeUnit.SECONDS);



                // TO open the URL

                String URL = "https://www.axisbank.com/retail/calculators/fd-calculator";

                driver.get(URL);

                Thread.sleep(3000);



        }



        public void Screenshot(String filename) throws Exception {



                // TO take screenshot and store it into file format

                File file = ((TakesScreenshot) driver).getScreenshotAs(OutputType.FILE);



                // Copy the screenshot and store it intodesired location

                FileUtils.copyFile(file, new File(

                                "/Users/759412/Desktop/OSCAR/RDP/Selenium SBA/SeleniumSBA/ClearTripScreen/" + filename + ".jpg"));



        }



        @Test

        public void RadioButton() throws Exception {



                // Disable the popup

                WebElement popup = driver.findElement(By.xpath("//*[@id='nvpush_cross']"));

                Actions action = new Actions(driver);

                action.moveToElement(popup).build().perform();

                popup.click();



                // Select the radio button Senior citizen

                WebDriverWait wait = new WebDriverWait(driver, 10);

                WebElement Senior = wait.until(

                                ExpectedConditions.elementToBeClickable(By.xpath("//*[@id='RadioButton']//label[@for='radio2']")));

                JavascriptExecutor jse = (JavascriptExecutor) driver;

                jse.executeScript("arguments[0].click();", Senior);



                // Select Monthaly payout from the DropDown Box

                Select TypeOfFD = new Select(driver.findElement(By.xpath("//*[@id='FdepType']")));

                TypeOfFD.selectByVisibleText("Monthly Payout");



                // Enter value 40000 into Amount depoist

                // First clear the Amount depoist



                WebElement AmountDepoist = driver.findElement(By.xpath("//*[@id='loan_amount']"));

                action.moveToElement(AmountDepoist).build().perform();

                AmountDepoist.clear();



                // To get AmountDepoist from excel

                System.out.println(getAmount());

                WebElement AmountToDepoist = driver.findElement(By.xpath("//*[@id='loan_amount']"));

                AmountToDepoist.sendKeys(getAmount());

                AmountToDepoist.sendKeys(Keys.TAB);

                // (By.xpath("//*[text()='19']")).click();



                // Print Rate of intrest

                String ROI = driver.findElement(By.xpath("//*[@id='matIntRate']")).getText();

                System.out.println(ROI);



                // To take screenshot of the page

                Screenshot("FDCalculator");



        }



        @AfterTest

        public void Teardown() {

                driver.quit();

        }



}





