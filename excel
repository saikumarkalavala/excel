package sel2;







import java.io.IOException;



import java.util.concurrent.TimeUnit;







import org.openqa.selenium.By;



import org.openqa.selenium.support.ui.Select;



import org.openqa.selenium.support.ui.WebDriverWait;







public class petco extends driversetup{







public static void main(String[] args) throws IOException, InterruptedException {



// TODO Auto-generated method stub



driver=driversetup.chromesetup();



WebDriverWait wait=new WebDriverWait(driver,3);



wait.withTimeout(1000, TimeUnit.SECONDS);



driver.findElement(By.xpath("(//a[@class='top-level has-subnav toggle-open'])[1]")).click();



Thread.sleep(3000);



driver.findElement(By.xpath("(//a[@class='subcategory'])[1]")).click();







driver.findElement(By.xpath("(//p[@class='text-center text-dark'])[1]")).click();



driver.findElement(By.xpath("//*[contains(text(),'Vet-Authorized Diet Dog Food (141)')]")).click();



Select d= new Select(driver.findElement(By.id("product-listing-sort-by_4_3074457345618259661_4099276460824443117")));



d.selectByValue("1");



String s1=driver.findElement(By.xpath("(//span[@class='brand-name'])[1]")).getText();



System.out.println(s1);



String p1=driver.findElement(By.xpath("(//span[@class='product-price-rddetail'])[1]")).getText();



System.out.println(p1);



driver.findElement(By.xpath("//label[@class='checkbox-button text-normal text-dark Between_$10_and_$25']")).click();



Thread.sleep(3000);



String s2=driver.findElement(By.xpath("(//span[@class='brand-name'])[2]")).getText();



System.out.println(s2);



String p2=driver.findElement(By.xpath("(//span[@class='product-price-rddetail'])[2]")).getText();



System.out.println(p2);



driver.findElement(By.xpath("(//span[@class='brand-name'])[2]")).click();



driversetup.Screenshot("shot5");



driver.quit();



}







}



